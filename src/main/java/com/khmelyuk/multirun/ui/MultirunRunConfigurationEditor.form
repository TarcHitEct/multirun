<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.khmelyuk.multirun.ui.MultirunRunConfigurationEditor">
  <grid id="27dc6" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="0" left="0" bottom="0" right="0"/>
    <constraints>
      <xy x="20" y="20" width="507" height="405"/>
    </constraints>
    <properties/>
    <border type="line">
      <color color="-10066330"/>
    </border>
    <children>
      <grid id="41447" binding="myMainPanel" layout-manager="GridLayoutManager" row-count="6" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="493" height="112"/>
          </grid>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <component id="253cc" class="javax.swing.JLabel">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Choose configurations to run:"/>
            </properties>
          </component>
          <component id="d2780" class="javax.swing.JCheckBox" binding="startOneByOne">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Start each configuration one by one"/>
            </properties>
          </component>
          <component id="c3f8" class="javax.swing.JCheckBox" binding="reuseTabs">
            <constraints>
              <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <actionCommand value=""/>
              <text value="Re-use tabs to run configurations"/>
              <toolTipText value="Enable re-using the tab of already completed run configuration for the newly started one."/>
            </properties>
          </component>
          <grid id="5c519" binding="collectionsPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="a3e3d" class="javax.swing.JCheckBox" binding="configurationsListChanged">
                <constraints border-constraint="South"/>
                <properties>
                  <enabled value="true"/>
                  <hideActionText value="true"/>
                  <text value=""/>
                </properties>
                <clientProperties>
                  <hideActionText class="java.lang.Boolean" value="true"/>
                  <html.disable class="java.lang.Boolean" value="false"/>
                </clientProperties>
              </component>
            </children>
          </grid>
          <component id="67a0f" class="javax.swing.JCheckBox" binding="markFailedProcess">
            <constraints>
              <grid row="4" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Mark the tab of failed run configuration"/>
              <toolTipText value="Add the icon to the tab with failed run configuration. This allows quickly visually identify the tab with failed applications or tests."/>
            </properties>
          </component>
          <component id="45507" class="javax.swing.JCheckBox" binding="hideSuccessProcess">
            <constraints>
              <grid row="5" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <selected value="false"/>
              <text value="Hide the tab of successfully completed run configuration"/>
              <toolTipText value="Allows to hide the tabs for the run configurations that completed successfully. Useful for test run configurations, to hide success tests, and leave open only failed tests."/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
